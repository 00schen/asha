<?xml version="1.0" ?>

<robot name="switch.urdf">
  <material name="Dark Grey">
    <color rgba=".3 .3 .3 1"/>
  </material>
  <material name="Light Grey">
    <color rgba=".5 .5 .5 1"/>
  </material>
  <link name="case_link">
    <inertial>
      <mass value="5"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy = "0 0 0"/>
      <geometry>
			  <mesh filename="case1.obj" scale="1 1 .75"/>
      </geometry>
      <material name="Dark Grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy = "0 0 0"/>
      <geometry>
			  <mesh filename="case1.obj" scale="1 1 .75"/>
      </geometry>
    </collision>
    
  </link>
  <link name="button_link">
    <inertial>
      <mass value="5"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="0 .27 0" rpy="0 0 0"/>
      <geometry>
			  <mesh filename="switch.obj" scale="5 5 3"/>
      </geometry>
      <material name="Light Grey"/>
    </visual>
    <collision>
      <origin xyz="0 .27 0" rpy="0 0 0"/>
      <geometry>
			  <mesh filename="switch.obj" scale="5 5 3"/>
      </geometry>
    </collision>
  </link>
  <joint name="hinge" type="revolute">
      <parent link="case_link"/>
      <child link="button_link"/>
      <origin xyz="0 -.3 0" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <limit lower="-1" upper=".1" effort="100" velocity="100"/>
  </joint>
</robot>